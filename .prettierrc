{
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 4,
    "bracketSameLine": true,
    "jsxSingleQuote": true,
    "singleAttributePerLine": true,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "strict",
    "endOfLine": "lf",
    "trailingComma": "none",
    "semi": true,
    "useTabs": false,
    "arrowParens": "always",
    "quoteProps": "as-needed"
}
